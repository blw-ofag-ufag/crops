@prefix : <https://lindas.admin.ch/foag/crops> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

:CropGroup a owl:Class ;
    rdfs:label "Kulturgruppe"@de .

:DirectPaymentCrop a owl:Class ;
    rdfs:label "Direktzahlungskultur"@de ;
    rdfs:subClassOf :CropGroup .

:BFFQ1 a owl:Class ;
    rdfs:label "BFF-Q1-Kultur"@de ;
    rdfs:subClassOf :CropGroup .

:SpecialtyCrop a owl:Class ;
    rdfs:label "Spezialkultur"@de ;
    rdfs:subClassOf :CropGroup .

:LNF a owl:DatatypeProperty ;
    rdfs:domain :CropGroup ;
    rdfs:range xsd:integer ;
    rdfs:label "LNF-Code"@de .
